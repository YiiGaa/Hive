{
    "configure":{
        "@root@":"..",
        "@ThinkRoot@":"/kernel/think",
        "@MoveRoot@":"/kernel/move",
        "@CommonRoot@":"/kernel/common",
        "@ConfigRoot@":"/kernel/config",
        "@TemplateRoot@":"/Christmas/Template/AutoLinkModule"
    },
    "control":{
        "generatePath":"@root@",
        "enginneringPath":"@root@",
        "directMerger":[
            {
                "targetPath":"@ThinkRoot@/think.hpp",
                "sourcePath":[
                    {"rootPath":"@TemplateRoot@","contain":"ThinkLink.tmpl","except":"_bak_","filter":"filter_HPP_Head"},
                    {"rootPath":"@ThinkRoot@","contain":".hpp","except":"think.hpp,interface.hpp,_bak_","filter":"filter_Linkpackage"},
                    {"rootPath":"@TemplateRoot@","contain":"ThinkLink.tmpl","except":"_bak_","filter":"filter_HPP_Tail"}
                ]
            },
            {
                "targetPath":"@ThinkRoot@/think.cpp",
                "sourcePath":[
                    {"rootPath":"@TemplateRoot@","contain":"ThinkLink.tmpl","except":"_bak_","filter":"filter_CPP_Head"},
                    {"rootPath":"@ThinkRoot@","contain":".hpp","except":"think.hpp,interface.hpp,_bak_","filter":"filter_LinkObject"},
                    {"rootPath":"@TemplateRoot@","contain":"ThinkLink.tmpl","except":"_bak_","filter":"filter_CPP_Tail"}
                ]
            },
            {
                "targetPath":"@MoveRoot@/move.hpp",
                "sourcePath":[
                    {"rootPath":"@TemplateRoot@","contain":"MoveLink.tmpl","except":"_bak_","filter":"filter_HPP_Head"},
                    {"rootPath":"@MoveRoot@","contain":".hpp","except":"move.hpp,interfaces.hpp,_bak_","filter":"filter_Linkpackage"},
                    {"rootPath":"@TemplateRoot@","contain":"MoveLink.tmpl","except":"_bak_","filter":"filter_HPP_Tail"}
                ]
            },
            {
                "targetPath":"@MoveRoot@/move.cpp",
                "sourcePath":[
                    {"rootPath":"@TemplateRoot@","contain":"MoveLink.tmpl","except":"_bak_","filter":"filter_CPP_Head"},
                    {"rootPath":"@MoveRoot@","contain":".hpp","except":"move.hpp,interfaces.hpp,_bak_","filter":"filter_LinkObject"},
                    {"rootPath":"@TemplateRoot@","contain":"MoveLink.tmpl","except":"_bak_","filter":"filter_CPP_Tail"}
                ]
            },
            {
                "targetPath":"@CommonRoot@/errorcode/errorcode.hpp",
                "sourcePath":[
                    {"rootPath":"@TemplateRoot@","contain":"ErrorCode.tmpl","except":"_bak_","filter":"filter_HPP_Head"},
                    {"rootPath":"@ThinkRoot@","contain":".hpp","except":"think.hpp,interface.hpp,_bak_","filter":"filter_ErrorEnum"},
                    {"rootPath":"@MoveRoot@","contain":".hpp","except":"move.hpp,interfaces.hpp,_bak_","filter":"filter_ErrorEnum"},
                    {"rootPath":"@TemplateRoot@","contain":"ErrorCode.tmpl","except":"_bak_","filter":"filter_HPP_Tail"}
                ]
            },
            {
                "targetPath":"@CommonRoot@/errorcode/errorcode.cpp",
                "sourcePath":[
                    {"rootPath":"@TemplateRoot@","contain":"ErrorCode.tmpl","except":"_bak_","filter":"filter_CPP_Head"},
                    {"rootPath":"@ThinkRoot@","contain":".hpp","except":"think.hpp,interface.hpp,_bak_","filter":"filter_ErrorCode"},
                    {"rootPath":"@MoveRoot@","contain":".hpp","except":"move.hpp,interfaces.hpp,_bak_","filter":"filter_ErrorCode"},
                    {"rootPath":"@TemplateRoot@","contain":"ErrorCode.tmpl","except":"_bak_","filter":"filter_CPP_Tail"}
                ]
            },
            {
                "targetPath":"@ConfigRoot@/config.hpp",
                "sourcePath":[
                    {"rootPath":"@TemplateRoot@","contain":"Config.tmpl","except":"_bak_","filter":"filter_HPP_Head"},
                    {"rootPath":"@CommonRoot@/logger","contain":"logger.hpp","except":"_bak_","filter":"filter_Config"},
                    {"rootPath":"@ThinkRoot@","contain":".hpp","except":"think.hpp,interface.hpp,_bak_","filter":"filter_Config"},
                    {"rootPath":"@MoveRoot@","contain":".hpp","except":"move.hpp,interfaces.hpp,_bak_","filter":"filter_Config"},
                    {"rootPath":"@TemplateRoot@","contain":"Config.tmpl","except":"_bak_","filter":"filter_HPP_Tail"}
                ]
            }
        ]
    }
}

